import { UpdateHostClassService } from 'ngx-tethys/core';
import { ElementRef, OnInit, TemplateRef } from '@angular/core';
import { ThyDialogFooterAlign, ThyDialogLayoutConfig } from '../dialog.config';
export declare class DialogFooterComponent implements OnInit {
    private dialogLayoutConfig;
    private updateHostClassService;
    private elementRef;
    description: TemplateRef<any>;
    set thyDivided(value: boolean);
    thyAlign: ThyDialogFooterAlign;
    private divided;
    private get align();
    constructor(dialogLayoutConfig: ThyDialogLayoutConfig, updateHostClassService: UpdateHostClassService, elementRef: ElementRef);
    ngOnInit(): void;
}
