import { OnInit, OnDestroy, NgZone } from '@angular/core';
import { NotifyPlacement, ThyNotifyOptions } from './notify-option.interface';
import { NotifyQueueStore } from './notify-queue.store';
export declare class ThyNotifyComponent implements OnInit, OnDestroy {
    private _queueStore;
    private _ngZone;
    flyInOut: string;
    className: string;
    option: ThyNotifyOptions;
    notifyIconName: string;
    extendContentClass: boolean;
    closeTimer: any;
    isShowDetail: boolean;
    placement: NotifyPlacement;
    set thyOption(value: ThyNotifyOptions);
    constructor(_queueStore: NotifyQueueStore, _ngZone: NgZone);
    ngOnInit(): void;
    ngOnDestroy(): void;
    extendContent(): void;
    showDetailToggle(): void;
    closeNotify(): void;
    triggerDetail(): void;
    mouseenter(): void;
    mouseleave(): void;
    private _creatCloseTimer;
    private _clearCloseTimer;
}
