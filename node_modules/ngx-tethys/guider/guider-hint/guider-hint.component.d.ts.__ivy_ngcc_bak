import { OnInit, TemplateRef } from '@angular/core';
import { ThyGuiderRef } from '../guider-ref';
import { ThyGuiderStep } from '../guider.class';
import { ThyGuiderStepRef } from '../guider-step-ref';
export declare class ThyGuiderHintComponent implements OnInit {
    guiderHint: boolean;
    guiderRef: ThyGuiderRef;
    stepRef: ThyGuiderStepRef;
    descriptionString: string;
    descriptionTemplateRef: TemplateRef<any>;
    currentStepIndex: number;
    constructor();
    ngOnInit(): void;
    private setDescription;
    jump(): void;
    prev(): void;
    next(): void;
    end(): void;
    trackByFn(index: number, step: ThyGuiderStep): string | number;
}
