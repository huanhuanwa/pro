import { Injectable } from '@angular/core';
export class ThyAvatarService {
    constructor() {
        this.ignoreAvatarSrcPaths = [`default.png`];
    }
}
export class ThyDefaultAvatarService extends ThyAvatarService {
    avatarSrcTransform(src, size) {
        return src;
    }
    nameTransform(name) {
        return name;
    }
}
ThyDefaultAvatarService.decorators = [
    { type: Injectable }
];
