import { coerceBooleanProperty } from 'ngx-tethys/util';
import { Component, HostBinding, Input } from '@angular/core';
export class ThyCardContentComponent {
    constructor() {
        this.thyCardContentClass = true;
        this.scrollClassName = false;
        this._thySizeSm = false;
    }
    set thyScroll(value) {
        this.scrollClassName = coerceBooleanProperty(value);
    }
    set thySize(value) {
        this._thySizeSm = value === 'sm';
    }
    ngOnInit() { }
}
ThyCardContentComponent.decorators = [
    { type: Component, args: [{
                selector: 'thy-card-content',
                preserveWhitespaces: false,
                template: `
        <ng-content></ng-content>
    `
            },] }
];
ThyCardContentComponent.propDecorators = {
    thyCardContentClass: [{ type: HostBinding, args: ['class.thy-card-content',] }],
    scrollClassName: [{ type: HostBinding, args: ['class.thy-card-content--scroll',] }],
    thyScroll: [{ type: Input, args: ['thyScroll',] }],
    _thySizeSm: [{ type: HostBinding, args: ['class.thy-card-content--sm',] }],
    thySize: [{ type: Input, args: ['thySize',] }]
};
