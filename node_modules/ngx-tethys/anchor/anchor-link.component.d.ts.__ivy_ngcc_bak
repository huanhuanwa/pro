import { Platform } from '@angular/cdk/platform';
import { ElementRef, OnDestroy, OnInit, Renderer2, TemplateRef } from '@angular/core';
import { ThyAnchorComponent } from './anchor.component';
export declare class ThyAnchorLinkComponent implements OnInit, OnDestroy {
    elementRef: ElementRef;
    private anchorComponent;
    private platform;
    private renderer;
    thyHref: string;
    title: string | null;
    titleTemplate?: TemplateRef<any>;
    set thyTitle(value: string | TemplateRef<void>);
    thyTemplate: TemplateRef<void>;
    linkTitle: ElementRef<HTMLAnchorElement>;
    constructor(elementRef: ElementRef, anchorComponent: ThyAnchorComponent, platform: Platform, renderer: Renderer2);
    ngOnInit(): void;
    getLinkTitleElement(): HTMLAnchorElement;
    setActive(): void;
    unsetActive(): void;
    goToClick(e: Event): void;
    ngOnDestroy(): void;
}
