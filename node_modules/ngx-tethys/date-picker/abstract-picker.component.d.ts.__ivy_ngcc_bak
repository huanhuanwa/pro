import { ChangeDetectorRef, EventEmitter, OnChanges, OnDestroy, OnInit, SimpleChanges } from '@angular/core';
import { ControlValueAccessor } from '@angular/forms';
import { Subject } from 'rxjs';
import { ThyPickerComponent } from './picker.component';
import { CompatibleDate, CompatibleValue, DateEntry, RangeEntry } from './standard-types';
export declare abstract class AbstractPickerComponent implements OnInit, OnChanges, OnDestroy, ControlValueAccessor {
    cdr: ChangeDetectorRef;
    thyValue: CompatibleValue | null;
    thyAllowClear: boolean;
    thyAutoFocus: boolean;
    thyDisabled: boolean;
    thyOpen: boolean;
    thyDisabledDate: (d: Date) => boolean;
    thyMinDate: Date | number;
    thyMaxDate: Date | number;
    thyPlaceHolder: string | string[];
    thyReadonly: boolean;
    thyOriginClassName: string;
    thyPanelClassName: string;
    thySize: 'lg' | 'md' | 'sm' | 'xs' | 'default';
    thyFormat: string;
    thyAutoStartAndEnd: boolean;
    thyDefaultPickerValue: CompatibleDate | number | null;
    thySuffixIcon: string;
    readonly thyOpenChange: EventEmitter<boolean>;
    picker: ThyPickerComponent;
    isRange: boolean;
    withTime: boolean;
    protected destroyed$: Subject<void>;
    protected isCustomPlaceHolder: boolean;
    private onlyEmitDate;
    protected originWithTime: boolean;
    get realOpenState(): boolean;
    initValue(): void;
    constructor(cdr: ChangeDetectorRef);
    ngOnInit(): void;
    ngOnChanges(changes: SimpleChanges): void;
    ngOnDestroy(): void;
    closeOverlay(): void;
    onValueChange(originalValue: CompatibleValue): void;
    setFormatRule(): void;
    onOpenChange(open: boolean): void;
    onChangeFn: (val: CompatibleDate | DateEntry | RangeEntry | number | null) => void;
    onTouchedFn: () => void;
    writeValue(originalValue: CompatibleDate): void;
    registerOnChange(fn: any): void;
    registerOnTouched(fn: any): void;
    setTimePickerState(withTime: boolean): void;
    setDisabledState(disabled: boolean): void;
    private setDefaultPlaceHolder;
    setValue(value: CompatibleDate): void;
}
