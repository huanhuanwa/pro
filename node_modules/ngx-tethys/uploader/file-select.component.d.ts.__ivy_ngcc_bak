import { ElementRef, OnInit, OnDestroy, EventEmitter } from '@angular/core';
import { ThyUploaderConfig } from './uploader.config';
import { FileSelectBaseComponent } from './file-select-base';
export declare class ThyFileSelectComponent extends FileSelectBaseComponent implements OnInit, OnDestroy {
    elementRef: ElementRef;
    defaultConfig: ThyUploaderConfig;
    private multiple;
    private acceptFolder;
    thyOnFileSelect: EventEmitter<any>;
    fileInput: ElementRef<HTMLInputElement>;
    set thyMultiple(value: boolean);
    set thyAcceptFolder(value: boolean);
    set thyAcceptType(value: Array<string> | string);
    set thySizeThreshold(value: number);
    click($event: Event): void;
    constructor(elementRef: ElementRef, defaultConfig: ThyUploaderConfig);
    selectFile($event: Event): void;
    ngOnInit(): void;
    ngOnDestroy(): void;
}
