import { ElementRef, OnInit, NgZone, OnDestroy, EventEmitter } from '@angular/core';
export declare class ThyScrollDirective implements OnInit, OnDestroy {
    private elementRef;
    private ngZone;
    private _destroyed;
    private _enable;
    private _initialled;
    private _subscription;
    private _elementScrolled;
    set thyEnable(value: boolean);
    thyOnScrolled: EventEmitter<ElementRef>;
    constructor(elementRef: ElementRef<any>, ngZone: NgZone);
    ngOnInit(): void;
    ngOnDestroy(): void;
    getElementRef(): ElementRef<HTMLElement>;
}
