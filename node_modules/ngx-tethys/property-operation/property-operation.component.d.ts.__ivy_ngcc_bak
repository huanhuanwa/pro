import { AfterContentInit, ElementRef, EventEmitter, OnInit, TemplateRef } from '@angular/core';
import { UpdateHostClassService, ThyTranslate } from 'ngx-tethys/core';
declare type ThyPropertyOperationTypes = 'primary' | 'success' | 'warning' | 'danger';
export declare class ThyPropertyOperationComponent implements OnInit, AfterContentInit {
    private thyTranslate;
    private updateHostClassService;
    private elementRef;
    private initialized;
    labelText: string;
    onlyHasTips: boolean;
    showClose: boolean;
    type: ThyPropertyOperationTypes;
    icon: string;
    value: string;
    labelHideWhenHasValue: boolean;
    thyOnRemove: EventEmitter<any>;
    thyClick: EventEmitter<Event>;
    _isPropertyOperation: boolean;
    operationIcon: TemplateRef<any>;
    contentElement: ElementRef;
    set thyLabelText(value: string);
    set thyValue(value: string);
    set thyLabelTextTranslateKey(value: string);
    set thyIcon(value: string);
    set thyShowClose(value: boolean);
    set thyLabelHasValue(value: boolean);
    set thyLabelHideWhenHasValue(value: boolean);
    set thyType(value: ThyPropertyOperationTypes);
    active: boolean;
    disabled: boolean;
    private setHostClass;
    private setOnlyHasTips;
    constructor(thyTranslate: ThyTranslate, updateHostClassService: UpdateHostClassService, elementRef: ElementRef);
    ngOnInit(): void;
    onclick(event: Event): void;
    ngAfterContentInit(): void;
    remove($event: Event): void;
}
export {};
