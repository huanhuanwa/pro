import { EventEmitter, TemplateRef, IterableDiffers, OnInit, DoCheck } from '@angular/core';
import { ThyTransferSelectEvent, ThyTransferItem, InnerTransferDragEvent } from './transfer.interface';
import { ThyTransferComponent } from './transfer.component';
import { CdkDragDrop } from '@angular/cdk/drag-drop';
export declare class ThyTransferListComponent implements OnInit, DoCheck {
    private root;
    private differs;
    lockItems: ThyTransferItem[];
    unlockItems: ThyTransferItem[];
    private _diff;
    private _lockDiff;
    private _unlockDiff;
    title: string;
    items: ThyTransferItem[];
    draggable: boolean;
    canLock: boolean;
    maxLock: number;
    template: TemplateRef<any>;
    contentRef: TemplateRef<any>;
    draggableUpdate: EventEmitter<InnerTransferDragEvent>;
    selectItem: EventEmitter<ThyTransferSelectEvent>;
    unselectItem: EventEmitter<ThyTransferSelectEvent>;
    hostClass: string;
    constructor(root: ThyTransferComponent, differs: IterableDiffers);
    ngOnInit(): void;
    private _combineTransferData;
    private _afterChangeItems;
    ngDoCheck(): void;
    lockListEnterPredicate: () => boolean;
    onSelectItem: (item: ThyTransferItem) => void;
    onUnselectItem: (item: ThyTransferItem) => void;
    drop(event: CdkDragDrop<ThyTransferItem[]>): void;
}
