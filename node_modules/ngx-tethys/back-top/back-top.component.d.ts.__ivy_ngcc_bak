import { OnInit, TemplateRef, EventEmitter, NgZone, ChangeDetectorRef, OnDestroy, OnChanges } from '@angular/core';
import { Platform } from '@angular/cdk/platform';
import { ThyScrollService } from 'ngx-tethys/core';
export declare class ThyBackTopComponent implements OnInit, OnDestroy, OnChanges {
    private doc;
    private thyScrollService;
    private platform;
    private cdr;
    private zone;
    classNames: boolean;
    thyTemplate?: TemplateRef<void>;
    thyVisibilityHeight: number;
    thyContainer?: string | HTMLElement;
    readonly thyClick: EventEmitter<boolean>;
    visibleChange: EventEmitter<boolean>;
    visible: boolean;
    private scrollListenerDestroy$;
    private target;
    constructor(doc: any, thyScrollService: ThyScrollService, platform: Platform, cdr: ChangeDetectorRef, zone: NgZone);
    ngOnInit(): void;
    clickBackTop(): void;
    private getTarget;
    private handleScroll;
    private registerScrollEvent;
    ngOnDestroy(): void;
    ngOnChanges(changes: any): void;
}
