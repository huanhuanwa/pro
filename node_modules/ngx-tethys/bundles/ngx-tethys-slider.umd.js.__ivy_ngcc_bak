            },
            enumerable: false,
            configurable: true
        });
        ThySliderComponent.prototype.ngOnInit = function () {
            this.verificationValues();
            this.verificationStepValue();
            this.toggleDisabled();
            if (this.value === null || this.value === undefined) {
                this.setValue(this.ensureValueInRange(null));
            }
        };
        ThySliderComponent.prototype.ngAfterViewInit = function () {
            this.registerMouseEventsListeners();
            this.toggleDisabled();
        };
        ThySliderComponent.prototype.writeValue = function (newValue) {
            this.setValue(this.ensureValueInRange(newValue));
        };
        ThySliderComponent.prototype.registerOnChange = function (fn) {
            this.onChangeCallback = fn;
        };
        ThySliderComponent.prototype.registerOnTouched = function (fn) {
            this.onTouchedCallback = fn;
        };
        ThySliderComponent.prototype.ngOnChanges = function (changes) {
            if (changes.hasOwnProperty('thyMin') || changes.hasOwnProperty('thyMax') || changes.hasOwnProperty('thyStep')) {
                this.verificationValues();
                this.verificationStepValue();
            }
        };
        ThySliderComponent.prototype.ngOnDestroy = function () {
            this.unsubscribeMouseListeners();
        };
        ThySliderComponent.prototype.verificationValues = function () {
            if (this.thyMin >= this.thyMax) {
                throw new Error('min value must less than max value.');
            }
        };
        ThySliderComponent.prototype.verificationStepValue = function () {
            if (this.thyStep <= 0 || !!!this.thyStep) {
                throw new Error('step value must be greater than 0.');
            }
            else if (Number.isInteger(this.thyStep) && (this.thyMax - this.thyMin) % this.thyStep) {
                throw new Error('(max - min) must be divisible by step.');
            }
        };
        ThySliderComponent.prototype.toggleDisabled = function () {
            if (this.thyDisabled) {
                this.unsubscribeMouseListeners();
            }
            else {
                this.subscribeMouseListeners(['start']);
            }
        };
        ThySliderComponent.prototype.setValue = function (value) {
            if (this.value !== value) {
                this.value = value;
                this.updateTrackAndPointer();
            }
            this.onChangeCallback(this.value);
        };
        ThySliderComponent.prototype.ensureValueInRange = function (value) {
            if (!this.valueMustBeValid(value)) {
                return this.thyMin;
            }
            return util.clamp(value, this.thyMin, this.thyMax);
        };
        ThySliderComponent.prototype.valueMustBeValid = function (value) {
            return !isNaN(typeof value !== 'number' ? parseFloat(value) : value);
        };
        ThySliderComponent.prototype.updateTrackAndPointer = function () {
            var offset = this.valueToOffset(this.value);
            this.updateStyle(offset / 100);
            this.cdr.markForCheck();
        };
        ThySliderComponent.prototype.valueToOffset = function (value) {
            return ((value - this.thyMin) * 100) / (this.thyMax - this.thyMin);
        };
        ThySliderComponent.prototype.updateStyle = function (offsetPercentage) {
            var percentage = Math.min(1, Math.max(0, offsetPercentage));
            var orientFields = this.thyVertical ? ['height', 'bottom'] : ['width', 'left'];
            this.sliderTrack.nativeElement.style[orientFields[0]] = percentage * 100 + "%";
            this.sliderPointer.nativeElement.style[orientFields[1]] = percentage * 100 + "%";
        };
        ThySliderComponent.prototype.unsubscribeMouseListeners = function (actions) {
            if (actions === void 0) { actions = ['start', 'move', 'end']; }
            if (actions.includes('start') && this.dragStartHandler) {
                this.dragStartHandler.unsubscribe();
                this.dragStartHandler = null;
            }
            if (actions.includes('move') && this.dragMoveHandler) {
                this.dragMoveHandler.unsubscribe();
                this.dragMoveHandler = null;
            }
            if (actions.includes('end') && this.dragEndHandler) {
                this.dragEndHandler.unsubscribe();
                this.dragEndHandler = null;
            }
        };
        ThySliderComponent.prototype.subscribeMouseListeners = function (actions) {
            if (actions === void 0) { actions = ['start', 'move', 'end']; }
            if (actions.includes('start') && this.dragStartListener && !this.dragStartHandler) {
                this.dragStartHandler = this.dragStartListener.subscribe(this.mouseStartMoving.bind(this));
            }
            if (actions.includes('move') && this.dragMoveListener && !this.dragMoveHandler) {
                this.dragMoveHandler = this.dragMoveListener.subscribe(this.mouseMoving.bind(this));
            }
            if (actions.includes('end') && this.dragEndListener && !this.dragEndHandler) {
                this.dragEndHandler = this.dragEndListener.subscribe(this.mouseStopMoving.bind(this));
            }
        };
        ThySliderComponent.prototype.mouseStartMoving = function (value) {
            this.pointerController(true);
            this.setValue(value);
        };
        ThySliderComponent.prototype.mouseMoving = function (value) {
            this.setValue(this.ensureValueInRange(value));
            this.cdr.markForCheck();
        };
        ThySliderComponent.prototype.mouseStopMoving = function () {
            this.pointerController(false);
            this.cdr.markForCheck();
            this.thyAfterChange.emit({ value: this.value });
        };
        ThySliderComponent.prototype.pointerController = function (movable) {
            if (movable) {
                this.subscribeMouseListeners(['move', 'end']);
            }
            else {
                this.unsubscribeMouseListeners(['move', 'end']);
            }
        };
        ThySliderComponent.prototype.registerMouseEventsListeners = function () {
            var _this = this;
            var orientField = this.thyVertical ? 'pageY' : 'pageX';
            this.dragStartListener = this.ngZone.runOutsideAngular(function () {
                return rxjs.fromEvent(_this.ref.nativeElement, 'mousedown').pipe(operators.tap(function (e) {
                    e.stopPropagation();
                    e.preventDefault();
                }), operators.pluck(orientField), operators.map(function (position) { return _this.mousePositionToAdaptiveValue(position); }));
            });
            this.dragEndListener = this.ngZone.runOutsideAngular(function () {
                return rxjs.fromEvent(document, 'mouseup');
            });
            this.dragMoveListener = this.ngZone.runOutsideAngular(function () {
                return rxjs.fromEvent(document, 'mousemove').pipe(operators.tap(function (e) {
                    e.stopPropagation();
                    e.preventDefault();
                }), operators.pluck(orientField), operators.distinctUntilChanged(), operators.map(function (position) { return _this.mousePositionToAdaptiveValue(position); }), operators.distinctUntilChanged(), operators.takeUntil(_this.dragEndListener));
            });
        };
        ThySliderComponent.prototype.mousePositionToAdaptiveValue = function (position) {
            var sliderStartPosition = this.getSliderPagePosition();
            var sliderLength = this.getRailLength();
            var ratio = this.convertPointerPositionToRatio(position, sliderStartPosition, sliderLength);
            var value = this.ratioToValue(ratio);
            return parseFloat(value.toFixed(this.getDecimals(this.thyStep)));
        };
        ThySliderComponent.prototype.getSliderPagePosition = function () {
            var rect = this.ref.nativeElement.getBoundingClientRect();
            var window = this.ref.nativeElement.ownerDocument.defaultView;
            var orientFields = this.thyVertical ? ['bottom', 'pageYOffset'] : ['left', 'pageXOffset'];
            return rect[orientFields[0]] + window[orientFields[1]];
        };
        ThySliderComponent.prototype.getRailLength = function () {
            var orientFiled = this.thyVertical ? 'clientHeight' : 'clientWidth';
            return this.sliderRail.nativeElement[orientFiled];
        };
        ThySliderComponent.prototype.convertPointerPositionToRatio = function (pointerPosition, startPosition, totalLength) {
            if (this.thyVertical) {
                return util.clamp((startPosition - pointerPosition) / totalLength, 0, 1);
            }
            return util.clamp((pointerPosition - startPosition) / totalLength, 0, 1);
        };
        ThySliderComponent.prototype.ratioToValue = function (ratio) {
            var value = (this.thyMax - this.thyMin) * ratio + this.thyMin;
            var step = this.thyStep;
            if (ratio === 0) {
                value = this.thyMin;
            }
            else if (ratio === 1) {
                value = this.thyMax;
            }
            else {
                value = Math.round(value / step) * step;
            }
            return util.clamp(value, this.thyMin, this.thyMax);
        };
        ThySliderComponent.prototype.getDecimals = function (value) {
            var valueString = value.toString();
            var integerLength = valueString.indexOf('.') + 1;
            return integerLength >= 0 ? valueString.length - integerLength : 0;
        };
        return ThySliderComponent;
    }());
