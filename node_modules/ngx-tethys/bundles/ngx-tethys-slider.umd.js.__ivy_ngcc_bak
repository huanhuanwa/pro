    var ThySliderComponent = /** @class */ (function () {
        function ThySliderComponent(cdr, ngZone, ref, updateHostClassService) {
            this.cdr = cdr;
            this.ngZone = ngZone;
            this.ref = ref;
            this.updateHostClassService = updateHostClassService;
