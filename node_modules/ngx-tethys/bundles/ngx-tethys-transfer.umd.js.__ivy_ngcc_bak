        ThyTransferListComponent.prototype.ngOnInit = function () {
            this._combineTransferData();
            if (this.canLock) {
                this._lockDiff = this.differs.find(this.lockItems).create();
                this._unlockDiff = this.differs.find(this.unlockItems).create();
            }
            else {
                this._unlockDiff = this.differs.find(this.unlockItems).create();
            }
            this._diff = this.differs.find(this.items).create();
        };
        ThyTransferListComponent.prototype._combineTransferData = function () {
            var _this = this;
            this.lockItems = [];
            this.unlockItems = [];
            if (this.canLock) {
                (this.items || []).forEach(function (item) {
                    if (item.isLock) {
                        _this.lockItems.push(item);
                    }
                    else {
                        _this.unlockItems.push(item);
                    }
                });
            }
            else {
                this.unlockItems = this.items;
            }
        };
        ThyTransferListComponent.prototype._afterChangeItems = function (changes, items) {
            // 数据发生变化时，更改order值
            changes.forEachAddedItem(function (record) {
                record.item.order = record.currentIndex;
            });
            changes.forEachRemovedItem(function () {
                items.forEach(function (item, index) {
                    item.order = index;
                });
            });
            changes.forEachMovedItem(function () {
                items.forEach(function (item, index) {
                    item.order = index;
                });
            });
        };
        ThyTransferListComponent.prototype.ngDoCheck = function () {
            var changes = this._diff.diff(this.items);
            if (changes) {
                this._afterChangeItems(changes, this.items);
                this._combineTransferData();
            }
            if (this._lockDiff) {
                var lockChanges = this._lockDiff.diff(this.lockItems);
                if (lockChanges) {
                    this._afterChangeItems(lockChanges, this.lockItems);
                }
            }
            var unlockChanges = this._unlockDiff.diff(this.unlockItems);
            if (unlockChanges) {
                this._afterChangeItems(unlockChanges, this.unlockItems);
            }
        };
        ThyTransferListComponent.prototype.drop = function (event) {
            if (event.previousContainer === event.container) {
                dragDrop.moveItemInArray(event.container.data, event.previousIndex, event.currentIndex);
            }
            else {
                dragDrop.transferArrayItem(event.previousContainer.data, event.container.data, event.previousIndex, event.currentIndex);
                (event.previousContainer.data || []).forEach(function (item) {
                    item.isLock = event.previousContainer.id === 'lock';
                });
                (event.container.data || []).forEach(function (item) {
                    item.isLock = event.container.id === 'lock';
                });
            }
            var dragEvent = {
                model: event.item.data,
                models: event.container.data,
                oldIndex: event.previousIndex,
