    var sizeClassMap = {
        lg: ['thy-empty-state', 'thy-empty-state--lg'],
        md: ['thy-empty-state'],
        sm: ['thy-empty-state', 'thy-empty-state--sm']
    };
    var sizeMap = {
        lg: {
            height: 168,
            offsetTop: 30,
            defaultMarginTop: 120 // 不自动计算默认的 top 距离
        },
        md: {
            height: 118,
            offsetTop: 20,
            defaultMarginTop: 10
        },
