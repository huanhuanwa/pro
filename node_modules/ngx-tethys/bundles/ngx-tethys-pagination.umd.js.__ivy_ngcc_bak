    var ThyPaginationComponent = /** @class */ (function () {
        function ThyPaginationComponent(paginationConfig, updateHostClassService, elementRef, cdr) {
            this.paginationConfig = paginationConfig;
            this.updateHostClassService = updateHostClassService;
            this.elementRef = elementRef;
            this.cdr = cdr;
            this.isTemplateRef = util.isTemplateRef;
            this.config = Object.assign({}, PaginationDefaultConfig, this.paginationConfig.main);
            this.disabled = false;
            this.pageIndexChange = new core.EventEmitter();
            this.pageChanged = new core.EventEmitter();
            this.pages = [];
            this.pageIndex = 1;
            this.range = { from: 0, to: 0 };
            this.firstIndex = 1;
            this.isHideOnSinglePage = false;
            this.initialized = false;
            this.isPaginationClass = true;
            // 是否显示范围和total
            this.showTotal = false;
            this.updateHostClassService.initializeElement(this.elementRef.nativeElement);
        }
        Object.defineProperty(ThyPaginationComponent.prototype, "thyPageIndex", {
            set: function (pageIndex) {
                this.pageIndex = pageIndex;
                if (this.initialized) {
                    this.setPageIndex(pageIndex);
                }
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(ThyPaginationComponent.prototype, "thyPageSize", {
            set: function (pageSize) {
                this.pageSize = pageSize;
                if (this.initialized) {
                    this.calculatePageCount();
                    this.initializePages(this.pageIndex, this.pageCount);
                    this.cdr.markForCheck();
                }
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(ThyPaginationComponent.prototype, "thyTotal", {
            set: function (total) {
                this.total = total;
                if (this.initialized) {
                    this.calculatePageCount();
                    this.initializePages(this.pageIndex, this.pageCount);
                    this.cdr.markForCheck();
                }
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(ThyPaginationComponent.prototype, "showQuickJumper", {
            set: function (value) {
                this.config.showQuickJumper = value;
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(ThyPaginationComponent.prototype, "size", {
            set: function (size) {
                this.updateHostClassService.addClass("thy-pagination-" + size);
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(ThyPaginationComponent.prototype, "maxCount", {
            set: function (value) {
                this.config.maxCount = value;
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(ThyPaginationComponent.prototype, "thyRangeCount", {
            set: function (value) {
                if (Number.isInteger(value)) {
                    this.config.rangeCount = value;
                    if (this.initialized) {
                        this.setMarginalCount(value);
                    }
                }
            },
            enumerable: false,
            configurable: true
        });
        ThyPaginationComponent.prototype.ngOnInit = function () {
            this.setMarginalCount(this.config.rangeCount);
            this.calculatePageCount();
            this.setPageIndex(this.pageIndex);
            this.initialized = true;
        };
        ThyPaginationComponent.prototype.setMarginalCount = function (range) {
            if (!this.marginalCount) {
                this.marginalCount = range <= DEFAULT_RANGE_COUNT ? 1 : 2;
            }
        };
        ThyPaginationComponent.prototype.setPageIndex = function (pageIndex) {
            this.pageIndex = pageIndex > this.pageCount ? this.pageCount : pageIndex || 1;
            this.range = {
                from: (this.pageIndex - 1) * this.pageSize + 1,
                to: this.pageIndex * this.pageSize
            };
            this.initializePages(this.pageIndex, this.pageCount);
            this.cdr.markForCheck();
        };
        ThyPaginationComponent.prototype.calculatePageCount = function () {
            var pageCount = this.pageSize < 1 ? 1 : Math.ceil(this.total / this.pageSize);
            this.pageCount = Math.max(pageCount || 0, 1);
        };
        ThyPaginationComponent.prototype.makePage = function (index, text, active) {
            return { index: index, text: text, active: active };
        };
        ThyPaginationComponent.prototype.initializePages = function (pageIndex, pageCount) {
            var marginalCount = this.marginalCount;
            var rangeCount = this.config.rangeCount;
            var maxCount = this.config.maxCount;
            var pages = [];
            var isMaxSized = pageCount > maxCount;
            if (isMaxSized) {
                var beforePages = [];
                var afterPages = [];
                // beforePages
                for (var i = 1; i <= marginalCount; i++) {
                    beforePages.push(this.makePage(i, i.toString(), i === pageIndex));
                }
                if (pageIndex - Math.ceil(rangeCount / 2) > this.firstIndex) {
                    beforePages.push(this.makePage(pageIndex - rangeCount, '...', null));
                }
                // afterPages
                if (pageIndex + Math.ceil(rangeCount / 2) < pageCount) {
                    afterPages.push(this.makePage(pageIndex + rangeCount, '...', null));
                }
                for (var i = pageCount - marginalCount + 1; i <= pageCount; i++) {
                    afterPages.push(this.makePage(i, i.toString(), i === pageIndex));
                }
                // mainPages
                var start = Math.max(marginalCount + 1, pageIndex - (rangeCount - 1) / 2);
                var end = Math.min(pageIndex + (rangeCount - 1) / 2, pageCount - marginalCount);
                if (pageIndex - 1 <= marginalCount) {
                    end = rangeCount;
                }
                if (pageCount - pageIndex <= marginalCount) {
                    start = pageCount - rangeCount + 1;
                }
                for (var i = start; i <= end; i++) {
                    pages.push({
                        index: i,
                        text: i.toString(),
                        active: i === +pageIndex
                    });
                }
                pages = __spread(beforePages, pages, afterPages);
            }
            else {
                for (var i = 1; i <= pageCount; i++) {
                    pages.push({
                        index: i,
                        text: i.toString(),
                        active: i === +pageIndex
                    });
                }
            }
            this.pages = pages;
        };
        ThyPaginationComponent.prototype.pageChange = function (pageIndex) {
            this.pageIndexChange.emit(pageIndex);
            this.pageChanged.emit({ page: pageIndex });
        };
        ThyPaginationComponent.prototype.selectPage = function (pageIndex) {
            if (this.disabled || pageIndex === this.firstIndex - 1 || pageIndex === this.pageCount + 1) {
                return;
            }
            this.setPageIndex(pageIndex);
            this.pageChange(this.pageIndex);
        };
        ThyPaginationComponent.prototype.jumpPage = function (input) {
            var pageIndex = +input.value;
            if (Number.isInteger(pageIndex)) {
                this.selectPage(pageIndex);
            }
            input.value = '';
        };
        return ThyPaginationComponent;
    }());
