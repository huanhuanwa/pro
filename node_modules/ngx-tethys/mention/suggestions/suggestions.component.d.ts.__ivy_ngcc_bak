import { OnInit, OnDestroy, ElementRef, NgZone } from '@angular/core';
import { Subject } from 'rxjs';
import { ThySelectionListChange } from 'ngx-tethys/list';
import { MentionDefaultDataItem, Mention, MentionSuggestionSelectEvent } from '../interfaces';
import { SeekQueryResult } from '../adapter/adapter';
import { ThyPopoverRef } from 'ngx-tethys/popover';
export declare class ThyMentionSuggestionsComponent<TItem = MentionDefaultDataItem> implements OnInit, OnDestroy {
    elementRef: ElementRef<HTMLElement>;
    private ngZone;
    private popoverRef;
    data: TItem[];
    mention: Mention<TItem>;
    suggestionSelect$: Subject<MentionSuggestionSelectEvent<any>>;
    debounce: number;
    loadingDone: boolean;
    private search$;
    suggestionsClass: boolean;
    constructor(elementRef: ElementRef<HTMLElement>, ngZone: NgZone, popoverRef: ThyPopoverRef<any>);
    ngOnInit(): void;
    search(query: SeekQueryResult): void;
    select(item: TItem, event: Event): void;
    selectionChange(event: ThySelectionListChange<TItem>): void;
    ngOnDestroy(): void;
}
