import { OnInit, TemplateRef } from '@angular/core';
import { ThySkeletonComponent } from '../skeleton.component';
export declare class ThySkeletonTitleComponent implements OnInit {
    private skeletonComponent;
    contentTemplateRef: TemplateRef<any>;
    width: number;
    height: number;
    set thyWidth(value: number);
    set thyHeight(value: number);
    constructor(skeletonComponent: ThySkeletonComponent);
    ngOnInit(): void;
}
