import { OnInit, SimpleChanges, OnChanges, TemplateRef } from '@angular/core';
export declare class ThySkeletonComponent implements OnInit, OnChanges {
    private platformId;
    addSkeletonClass: boolean;
    thyAnimate: boolean;
    thyBaseUrl: string;
    thyWidth: string | number;
    thyHeight: string | number;
    thyViewBoxWidth: string | number;
    thyViewBoxHeight: string | number;
    thySpeed: number;
    thyPreserveAspectRatio: string;
    thyPrimaryColor: string;
    thySecondaryColor: string;
    thyPrimaryOpacity: number;
    thySecondaryOpacity: number;
    thyUniqueKey: string;
    thyRtl: string;
    thyStyle: {
        [key: string]: string;
    };
    thyIgnoreBaseUrl: boolean;
    thyLoadingDone: boolean;
    customTemplate: TemplateRef<any>;
    contentTemplates: TemplateRef<any>[];
    idClip: string;
    idGradient: string;
    defaultAnimation: string[];
    rtlAnimation: string[];
    animationValues: string[];
    fillStyle: {
        fill: string;
    };
    clipPath: string;
    constructor(platformId: string);
    ngOnInit(): void;
    ngOnChanges(changes: SimpleChanges): void;
    setFillStyle(): void;
    setClipPath(): void;
    assignInputProperties(input: ThySkeletonComponent): void;
    addTemplate(template: TemplateRef<any>): void;
}
