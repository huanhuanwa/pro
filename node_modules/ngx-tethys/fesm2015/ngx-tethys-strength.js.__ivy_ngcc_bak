import { Component, forwardRef, HostBinding, Input, NgModule } from '@angular/core';
import { NG_VALUE_ACCESSOR } from '@angular/forms';
import { ThyTranslate } from 'ngx-tethys/core';

var ThyStrengthEnum;
(function (ThyStrengthEnum) {
    ThyStrengthEnum[ThyStrengthEnum["highest"] = 4] = "highest";
    ThyStrengthEnum[ThyStrengthEnum["high"] = 3] = "high";
    ThyStrengthEnum[ThyStrengthEnum["average"] = 2] = "average";
    ThyStrengthEnum[ThyStrengthEnum["low"] = 1] = "low";
})(ThyStrengthEnum || (ThyStrengthEnum = {}));
const strengthMap = {
    [ThyStrengthEnum.highest]: {
        level: 'highest',
        text: '最高'
    },
    [ThyStrengthEnum.high]: {
        level: 'high',
        text: '高'
    },
    [ThyStrengthEnum.average]: {
        level: 'average',
        text: '中'
    },
    [ThyStrengthEnum.low]: {
        level: 'low',
        text: '低'
    }
};
class ThyStrengthComponent {
    constructor(translate) {
        this.translate = translate;
        this.styleClass = true;
        this.strengthMap = JSON.parse(JSON.stringify(strengthMap));
        this._onChange = Function.prototype;
        this._onTouched = Function.prototype;
    }
    set titleKey(value) {
        this.strengthTitle = this.translate.instant(value);
    }
    set highestKey(value) {
        this.strengthMap[ThyStrengthEnum.highest].text = this.translate.instant(value);
    }
    set highKey(value) {
        this.strengthMap[ThyStrengthEnum.high].text = this.translate.instant(value);
    }
    set averageKey(value) {
        this.strengthMap[ThyStrengthEnum.average].text = this.translate.instant(value);
    }
    set lowKey(value) {
        this.strengthMap[ThyStrengthEnum.low].text = this.translate.instant(value);
    }
    ngOnInit() { }
    writeValue(value) {
        this.strength = value;
    }
    registerOnChange(fn) {
        this._onChange = fn;
    }
    registerOnTouched(fn) {
        this._onTouched = fn;
    }
}
ThyStrengthComponent.decorators = [
    { type: Component, args: [{
                selector: 'thy-strength',
                template: "<div>{{strengthTitle}} <span class=\"strength-text strength-text-{{strengthMap[strength]?.level}}\">{{strengthMap[strength]?.text}}</span></div>\n<div class=\"strength-level strength-level-{{strengthMap[strength]?.level}}\">\n    <span></span>\n</div>\n",
                providers: [
                    {
                        provide: NG_VALUE_ACCESSOR,
                        useExisting: forwardRef(() => ThyStrengthComponent),
                        multi: true
                    }
                ]
            },] }
];
ThyStrengthComponent.ctorParameters = () => [
    { type: ThyTranslate }
];
ThyStrengthComponent.propDecorators = {
    styleClass: [{ type: HostBinding, args: ['class.password-strength-container',] }],
    titleKey: [{ type: Input }],
    highestKey: [{ type: Input }],
    highKey: [{ type: Input }],
    averageKey: [{ type: Input }],
    lowKey: [{ type: Input }]
};

class ThyStrengthModule {
}
ThyStrengthModule.decorators = [
    { type: NgModule, args: [{
                imports: [],
                exports: [ThyStrengthComponent],
                declarations: [ThyStrengthComponent],
                providers: []
            },] }
];

/**
 * Generated bundle index. Do not edit.
 */

export { ThyStrengthComponent, ThyStrengthModule };
//# sourceMappingURL=ngx-tethys-strength.js.map
