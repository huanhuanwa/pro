import { OnInit, ElementRef, Renderer2, TemplateRef } from '@angular/core';
import { ComponentType } from '@angular/cdk/portal';
import { ThyPopover } from 'ngx-tethys/popover';
export declare class ThyMenuItemActionComponent implements OnInit {
    private popover;
    private renderer;
    private elementRef;
    _boundEvent: boolean;
    _actionMenu: ComponentType<any> | TemplateRef<any>;
    isThyMenuItemIconMore: boolean;
    set thyActionMenu(value: ComponentType<any> | TemplateRef<any>);
    thyStopPropagation: boolean;
    private bindClickEvent;
    constructor(popover: ThyPopover, renderer: Renderer2, elementRef: ElementRef);
    ngOnInit(): void;
}
