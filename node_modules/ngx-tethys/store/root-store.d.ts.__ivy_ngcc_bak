import { Store } from './store';
export declare type StoreInstanceMap = Map<string, Store<any>>;
/**
 * @internal
 */
export declare class RootStore {
    private connectSuccessed;
    /**
     * 数据流 数据是一个Map，k,v键值对，关键字->状态数据
     */
    private readonly _containers;
    private _plugin;
    private _combinedStateSubscription;
    constructor();
    private _assignCombinedState;
    /**
     * 合并数据流
     * 合并状态数据，把状态数据转换为这样的数据：{ containerName: string, state: any }，并且
     * 通过combineLatest合并成一个数据数据流，这样状态数据只有涉及更新，那么这边就会得到通知
     * @param containers 状态数据的Map
     */
    private _getCombinedState;
    /**
     * @internal
     */
    ngOnDestroy(): void;
    /**
     * @internal
     */
    registerStore(store: Store<any>): void;
    existStoreInstanceId(instanceId: string): boolean;
    /**
     * @internal
     */
    unregisterStore(store: Store<any>): void;
}
export declare function getSingletonRootStore(): RootStore;
