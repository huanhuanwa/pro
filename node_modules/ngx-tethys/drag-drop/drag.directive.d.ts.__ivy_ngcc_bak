import { NgZone, ElementRef, OnDestroy } from '@angular/core';
import { DragRef } from './drag-ref';
import { ThyDragDropService } from './drag-drop.service';
import { MixinBase, Constructor, ThyUnsubscribe } from 'ngx-tethys/core';
import { IThyDropContainerDirective } from './drop-container.class';
declare const _MixinBase: Constructor<ThyUnsubscribe> & typeof MixinBase;
export declare class ThyDragDirective<T = any> extends _MixinBase implements OnDestroy {
    container: IThyDropContainerDirective;
    private elementRef;
    set dragData(data: T);
    data: T;
    isDraggable: boolean;
    private _disabled;
    set disabled(isDisabled: boolean);
    get disabled(): boolean;
    dragRef: DragRef<T>;
    constructor(document: any, container: IThyDropContainerDirective, ngZone: NgZone, elementRef: ElementRef<HTMLElement>, service: ThyDragDropService<T>);
    ngOnDestroy(): void;
}
export {};
