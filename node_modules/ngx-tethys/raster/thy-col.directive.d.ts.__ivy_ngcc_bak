import { OnChanges, ElementRef, AfterViewInit, OnDestroy, Renderer2, OnInit } from '@angular/core';
import { UpdateHostClassService } from 'ngx-tethys/core';
import { ThyRowDirective } from './thy-row.directive';
import { MixinBase, Constructor, ThyUnsubscribe } from 'ngx-tethys/core';
export interface ThyColEmbeddedProperty {
    span?: number;
    pull?: number;
    push?: number;
    offset?: number;
    order?: number;
}
declare const _MixinBase: Constructor<ThyUnsubscribe> & typeof MixinBase;
export declare class ThyColDirective extends _MixinBase implements OnInit, OnChanges, AfterViewInit, OnDestroy {
    private elementRef;
    private renderer;
    private updateHostClassService;
    thyRowDirective: ThyRowDirective;
    thyFlex: string | number | null;
    thySpan: number | null;
    thyOrder: number | null;
    thyOffset: number | null;
    thyPush: number | null;
    thyPull: number | null;
    thyXs: number | ThyColEmbeddedProperty | null;
    thySm: number | ThyColEmbeddedProperty | null;
    thyMd: number | ThyColEmbeddedProperty | null;
    thyLg: number | ThyColEmbeddedProperty | null;
    thyXl: number | ThyColEmbeddedProperty | null;
    thyXXl: number | ThyColEmbeddedProperty | null;
    constructor(elementRef: ElementRef, renderer: Renderer2, updateHostClassService: UpdateHostClassService, thyRowDirective: ThyRowDirective);
    ngOnInit(): void;
    ngOnChanges(changes: import('@angular/core').SimpleChanges): void;
    ngAfterViewInit(): void;
    private _setHostClassMap;
    ngOnDestroy(): void;
}
export {};
