import { ElementRef, QueryList, TemplateRef } from '@angular/core';
import { ThyProgressTypes, ThyStackedValue } from './interfaces';
import { UpdateHostClassService } from 'ngx-tethys/core';
import { ThyProgressBarComponent, ThyParentProgress } from './bar/progress-bar.component';
export declare class ThyProgressComponent implements ThyParentProgress {
    private updateHostClassService;
    value: number | ThyStackedValue[];
    bars: ThyProgressBarComponent[];
    max: number;
    isStacked: boolean;
    isProgress: boolean;
    set barsQueryList(value: QueryList<ThyProgressBarComponent>);
    thyType: ThyProgressTypes;
    thyTips: string | TemplateRef<HTMLElement>;
    set thyValue(value: number | ThyStackedValue[]);
    set thySize(size: string);
    set thyMax(max: number);
    constructor(updateHostClassService: UpdateHostClassService, elementRef: ElementRef);
}
