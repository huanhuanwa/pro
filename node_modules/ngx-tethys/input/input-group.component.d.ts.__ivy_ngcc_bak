import { TemplateRef, ElementRef } from '@angular/core';
import { ThyTranslate, UpdateHostClassService } from 'ngx-tethys/core';
export declare type InputGroupSize = 'sm' | 'lg' | '';
export declare class ThyInputGroupComponent {
    private thyTranslate;
    private updateHostClassService;
    private elementRef;
    appendText: string;
    prependText: string;
    _isInputGroup: boolean;
    set thyAppendText(value: string);
    set thyAppendTextTranslateKey(value: string);
    set thyPrependText(value: string);
    set thyPrependTextTranslateKey(value: string);
    set thySize(size: InputGroupSize);
    appendTemplate: TemplateRef<any>;
    prependTemplate: TemplateRef<any>;
    constructor(thyTranslate: ThyTranslate, updateHostClassService: UpdateHostClassService, elementRef: ElementRef);
}
