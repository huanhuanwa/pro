import { ElementRef, OnInit, Renderer2 } from '@angular/core';
import { UpdateHostClassService } from 'ngx-tethys/core';
export declare type InputSize = 'xs' | 'sm' | 'md' | 'lg' | '';
export declare class ThyInputDirective implements OnInit {
    private updateHostClassService;
    private elementRef;
    private render;
    _isFormControl: boolean;
    private autocomplete;
    private initialized;
    set thySize(size: InputSize);
    set thyAutocomplete(value: boolean);
    constructor(updateHostClassService: UpdateHostClassService, elementRef: ElementRef, render: Renderer2);
    ngOnInit(): void;
    private setAutocomplete;
}
