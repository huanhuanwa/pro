import { OnInit, TemplateRef } from '@angular/core';
declare type ThyResultStatus = 'success' | 'warning' | 'error';
export declare class ThyResultComponent implements OnInit {
    thyStatus: ThyResultStatus;
    thyTitle: string;
    thySubtitle: string;
    thyIcon: string;
    iconTemplateRef: TemplateRef<any>;
    extraTemplateRef: TemplateRef<any>;
    titleTemplateRef: TemplateRef<any>;
    subtitleTemplateRef: TemplateRef<any>;
    className: boolean;
    constructor();
    ngOnInit(): void;
}
export {};
